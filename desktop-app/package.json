{
  "name": "dallol-bingo",
  "version": "1.0.0",
  "description": "Dallol Bingo Electron application",
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "start": "electron-vite preview",
    "pack": "electron-vite build && electron-builder",
    "build:win": "electron-vite build && electron-builder --win",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "build": {
    "appId": "com.bingo.app",
    "productName": "Dallol Bingo",
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**/*",
      "src/main/server/**/*",
      "package.json",
      "build/icon.png",
      "build/icon.ico"
    ],
    "linux": {
      "icon": "build/icon.png",
      "target": [
        "AppImage"
      ],
      "category": "Games"
    },
    "win": {
      "icon": "build/icon.ico",
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Dallol Bingo"
    },
    "publish": null
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "devtron": "^1.4.0",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "electron-vite": "^5.0.0"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@electron/remote": "^2.1.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@mui/system": "^7.3.7",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "body-parser": "^2.2.2",
    "chart.js": "^4.5.1",
    "classnames": "^2.5.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "matter-js": "^0.20.0",
    "mongoose": "^9.1.4",
    "nedb": "^1.8.0",
    "papaparse": "^5.5.3",
    "react": "^19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-confetti": "^6.4.0",
    "react-datepicker": "^9.1.0",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.12.0",
    "react-select": "^5.10.2",
    "react-spinners": "^0.17.0",
    "react-toastify": "^11.0.5",
    "recharts": "^3.6.0",
    "tailwindcss": "^4.1.18",
    "winston": "^3.19.0",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.10"
  }
}